{
	"extends": "airbnb-base",
	"plugins": [
		"vue"
	],
	"globals": {
		"_": true,
		"axios": true,
		"document": true,
		"trans": true,
		"window": true
	},
	"rules": {
		"func-names": "off",
		"import/no-extraneous-dependencies": "off",
		"indent": [
			"error",
			"tab"
		],
		"no-console": [
			"error",
			{
				"allow": [
					"error"
				]
			}
		],
		"no-tabs": "off",
		"object-shorthand": "off",
		"one-var": "off",
		"padded-blocks": "off"
	}
}
